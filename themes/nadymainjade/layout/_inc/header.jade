header.site-header
    nav
        ul
            li
                if theme.logo
                    img(src=theme.root + theme.logo alt="")
            li
                != theme.title
                //a(href=url_for())=theme.title
            li
                if config.subtitle
                    != config.subtitle

    nav
        ul 
            - for (var i in theme.menu)
                li
                    a(href=url_for(theme.menu[i]))=i
